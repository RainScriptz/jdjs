local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Easy Scriptz", HidePremium = false, SaveConfig = true, IntroText = "Easy Scriptz", ConfigFolder = "OrionTest"})
local m_Library =
require(game.ReplicatedStorage:WaitForChild("Framework"):WaitForChild("Library"));
while not m_Library.Loaded do
    game:GetService("RunService").Heartbeat.Wait();
end

hookfunction(debug.getupvalue(m_Library.Network.Invoke, 1), function(...) return true end)

function EasterEggHunt()
    for i, v in pairs(workspace["__MAP"].EasterEggs:GetChildren()) do
        if v:GetAttribute("TextureIDX") then
            print(v.Name, v:GetAttribute("TextureIDX"))
            m_Library.Network.Invoke("Easter Egg Hunt: Claim", v.Name, v:GetAttribute("TextureIDX"))
        end
    end
end

function Started()
    OrionLib:MakeNotification({
      Name = "Succesfully!",
      Content = "Started egg opening",
      Image = "rbxassetid://4483345998",
      Time = 2
    })
end

local AutoFarm = Window:MakeTab({
    Name = "AutoFarm",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

AutoFarm:AddLabel("Credits: kuzja")
AutoFarm:AddLabel("When Script Stop Working; thats mean it finished hatching")
AutoFarm:AddLabel("eggs in this world, you have to change world.")
AutoFarm:AddLabel("Example changing world from Spawn World to Fantasy.")
AutoFarm:AddLabel("")


AutoFarm:AddButton({
    Name = "Auto Collect Egg Hunt",
    Callback = function()
              EasterEggHunt()
            Started()
      end    
})

OrionLib:Init()
